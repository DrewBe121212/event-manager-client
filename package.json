{
  "name": "event-manager",
  "version": "0.0.2",
  "description": "Event/Video Production - Inventory Management Application",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DrewBe121212/event-video-production-event-manager.git"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "build": "webpack -d --watch --config ./conf/webpack-dev.config.js",
    "build:dist": "npm run clean:dist && webpack -p --config ./conf/webpack-dist.config.js",
    "start": "npm run build & npm run start-server",
    "start:dist": "npm run build:dist && npm run start-server:dist",
    "start-server": "webpack-dev-server --config ./conf/webpack-dev.config.js",
    "start-server:dist": "webpack-dev-server --config ./conf/webpack-dist.config.js",
    "eslint": "eslint ./src/client/app/.",
    "eslint-fix": "eslint ./src/client/app/. --fix"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-eslint": "^8.0.3",
    "concurrently": "^3.5.1",
    "eslint": "^4.13.0",
    "eslint-plugin-react": "^7.5.1",
    "formik": "^0.10.5",
    "material-ui": "^1.0.0-beta.23",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.19.3",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "postcss-modules-values": "^1.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.4",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "redux-freeze": "^0.1.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.4.1",
    "webpack-auto-inject-version": "^1.0.0",
    "webpack-dev-server": "^2.6.1"

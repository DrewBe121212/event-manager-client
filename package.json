{
  "name": "event-manager",
  "version": "0.0.3",
  "description": "Event/Video Production - Inventory Management Application",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DrewBe121212/event-video-production-event-manager.git"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "build": "webpack -d --watch --config ./conf/webpack-dev.config.js",
    "build:dist": "npm run clean:dist && webpack --config ./conf/webpack-dist.config.js --define process.env.NODE_ENV='production'",
    "start": "npm run build & npm run start-server",
    "start:dist": "npm run build:dist && npm run start-server:dist",
    "start-server": "webpack-dev-server --config ./conf/webpack-dev.config.js",
    "start-server:dist": "webpack-dev-server --config ./conf/webpack-dist.config.js",
    "eslint": "eslint ./src/client/app/.",
    "eslint:fix": "eslint ./src/client/app/. --fix"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 5%"
            ],
            "node": "current"
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-async-generator-functions",
      "transform-class-properties"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "concurrently": "^3.5.1",
    "formik": "^0.11.5",
    "material-ui": "^1.0.0-beta.32",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.20.1",
    "postcss": "^6.0.17",
    "postcss-cssnext": "^3.1.0",
    "postcss-modules-values": "^1.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.8",
    "eslint": "^4.10.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.10",
    "redux-freeze": "^0.1.5",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.0",
    "webpack": "^3.4.1",
    "webpack-auto-inject-version": "^1.0.0",
    "webpack-dev-server": "^2.10.0"
  }
}
